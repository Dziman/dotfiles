<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
  </component>
  <component name="SpringEditorOptions">
    <option name="showMultipleContextsPanel" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
      <option name="EFFECT_TYPE" value="-1" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="ff0099" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff6666" />
      <option name="EFFECT_TYPE" value="-1" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="false" pattern="\bTODO:Write JavaDoc\b.*">
      <option name="FOREGROUND" value="cccc" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ff0000" />
      <option name="ERROR_STRIPE_COLOR" value="cccc" />
      <option name="EFFECT_TYPE" value="2" />
    </pattern>
    <pattern icon="default" useCustomColors="true" case-sensitive="false" pattern="\b:todo:\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <filter name="JavaDocs">
      <pattern index="2" />
      <pattern index="0" />
      <pattern index="1" />
    </filter>
    <filter name="FIXMEs">
      <pattern index="1" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>